
-- Orion UI Library
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "BroHub Orion", HidePremium = false, SaveConfig = true, ConfigFolder = "BroHub"})
OrionLib:Init()

-- Rayfield UI Library
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({Name = "BroHub Rayfield"})
Rayfield:Notify({Title = "BroHub", Content = "Rayfield UI Loaded!"})

-- Linoria UI Library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/violin-suzutsuki/LinoriaLib/main/Library.lua"))()
local Window = Library:CreateWindow('BroHub Linoria')

-- Kavo UI Library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("BroHub Kavo", "DarkTheme")

-- Material UI Library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()
local UI = Library.Load({Title = "BroHub Material", Style = 1, SizeX = 400, SizeY = 300})

-- Wally UI
