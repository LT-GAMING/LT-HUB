-- Hydra UI
loadstring(game:HttpGet("https://pastebin.com/raw/1QF0y3CJ"))()-- BroHub Multi-UI Selector
-- Script cho phép chọn UI Library để load

local UIS = game:GetService("UserInputService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Tạo ScreenGui
local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
ScreenGui.Name = "BroHubUISelector"

-- Khung chính
local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0, 250, 0, 300)
Frame.Position = UDim2.new(0.5, -125, 0.5, -150)
Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Frame.Active = true
Frame.Draggable = true

-- Tiêu đề
local Title = Instance.new("TextLabel", Frame)
Title.Text = "BroHub UI Selector"
Title.Size = UDim2.new(1, 0, 0, 40)
Title.TextColor3 = Color3.fromRGB(255,255,255)
Title.BackgroundColor3 = Color3.fromRGB(40,40,40)
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 18

-- Hàm tạo nút
local function makeButton(text, order, callback)
    local Button = Instance.new("TextButton", Frame)
    Button.Text = text
    Button.Size = UDim2.new(1, -20, 0, 30)
    Button.Position = UDim2.new(0, 10, 0, 50 + (order * 35))
    Button.BackgroundColor3 = Color3.fromRGB(50,50,50)
    Button.TextColor3 = Color3.fromRGB(255,255,255)
    Button.Font = Enum.Font.SourceSans
    Button.TextSize = 16
    Button.MouseButton1Click:Connect(callback)
end

-- Danh sách UI
makeButton("Orion UI", 0, function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
end)

makeButton("Rayfield UI", 1, function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
end)

makeButton("Linoria UI", 2, function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/violin-suzutsuki/LinoriaLib/main/Library.lua"))()
end)

makeButton("Kavo UI", 3, function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
end)

makeButton("Material UI", 4, function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()
end)

makeButton("Wally UI", 5, function()
    loadstring(game:HttpGet("https://pastebin.com/raw/1p6xnBNf"))()
end)

makeButton("Dex Explorer", 6, function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/infyiff/backup/main/Dex.lua"))()
end)

