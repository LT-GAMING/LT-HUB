repeat task.wait() until game:IsLoaded()
repeat task.wait() until game:GetService("Players").LocalPlayer and game:GetService("Players").LocalPlayer.Character

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")
local TweenService = game:GetService("TweenService")
local VirtualUser = game:GetService("VirtualUser")
local LocalPlayer = Players.LocalPlayer

-- Anti-AFK
LocalPlayer.Idled:Connect(function()
    VirtualUser:Button2Down(Vector2.new(0, 0), Workspace.CurrentCamera.CFrame)
    task.wait(0.009)
    VirtualUser:Button2Up(Vector2.new(0, 0), Workspace.CurrentCamera.CFrame)
end)

-- Load GUI
local lib = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local window = lib:CreateWindow({
    Title = "LT GAMING HUB",
    SubTitle = "Ultimate Full Farm Edition",
    TabWidth = 180,
    Theme = "Dark",
    Size = UDim2.fromOffset(600, 400),
    MinimizeKey = Enum.KeyCode.RightShift
})

-- Add Logo to GUI
local logo = Instance.new("ImageLabel")
logo.Image = "rbxassetid://112045466994309"
logo.Size = UDim2.new(0, 100, 0, 100)
logo.Position = UDim2.new(1, -110, 0, 10)
logo.BackgroundTransparency = 1
logo.Parent = window.Window

-- Tabs
local Tabs = {
    Farm = window:AddTab({ Title = "Farm" }),
    Boss = window:AddTab({ Title = "Boss" }),
    Chest = window:AddTab({ Title = "Chest" }),
    Fruit = window:AddTab({ Title = "Fruit" }),
    PvP = window:AddTab({ Title = "PvP" }),
    Raid = window:AddTab({ Title = "Raid" }),
    Teleport = window:AddTab({ Title = "Teleport" }),
    Stats = window:AddTab({ Title = "Stats" }),
    Settings = window:AddTab({ Title = "Settings" }),
}

-- Add dummy buttons to show working
Tabs.Farm:AddToggle("Auto Farm", { Default = false, Callback = function(v) print("Auto Farm:", v) end })
Tabs.Farm:AddInput("Select Monster", { Placeholder = "Bandit", Callback = function(v) print("Selected:", v) end })

Tabs.Boss:AddButton("Farm Kaido (Kata)", function() print("Kaido Farm triggered") end)
Tabs.Boss:AddButton("Farm Bone", function() print("Bone Farm triggered") end)

Tabs.Chest:AddToggle("Auto Chest Fly", { Default = false, Callback = function(v) print("Chest Fly:", v) end })
Tabs.Chest:AddToggle("Auto Chest Bypass", { Default = false, Callback = function(v) print("Chest Bypass:", v) end })

Tabs.Fruit:AddToggle("Auto Roll Fruit", { Default = false, Callback = function(v) print("Roll:", v) end })
Tabs.Fruit:AddToggle("Auto Store Fruit", { Default = false, Callback = function(v) print("Store:", v) end })

Tabs.PvP:AddToggle("Kill Aura", { Default = false, Callback = function(v) print("Aura:", v) end })

Tabs.Raid:AddDropdown("Raid Type", {
    Values = {"Flame", "Ice", "Quake", "Light"},
    Default = "Flame",
    Callback = function(v) print("Raid:", v) end
})
Tabs.Raid:AddButton("Auto Raid", function() print("Start Raid") end)

Tabs.Teleport:AddButton("Go to Sea 1", function() LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,10,0) end)
Tabs.Teleport:AddButton("Go to Sea 2", function() LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(100,10,0) end)
Tabs.Teleport:AddButton("Go to Sea 3", function() LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(200,10,0) end)

Tabs.Stats:AddDropdown("Stat Type", {
    Values = {"Melee", "Defense", "Sword", "Gun", "Blox Fruit"},
    Default = "Melee",
    Callback = function(v) print("Stats:", v) end
})

Tabs.Settings:AddLabel("Anti-AFK & Delay: 0.009s TNG Optimized")

-- Notify
lib:Notify({
    Title = "LT Gaming Hub",
    Content = "Ultimate Farm + Logo Loaded! LETâ€™S GO!",
    Duration = 6
})
